import threading
import asyncio
from userbot.events import register

async def pizda(am):
  count = int(am.pattern_match.group(1))
  for _ in range(count):
    await am.respond("ğŸ˜ˆ")

@register(outgoing=True, pattern="^.vam-pizda(.*)")

async def start():
  for _ in range(5):
    haha = threading.Thread(target=pizda)
    haha.start()
